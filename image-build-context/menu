default menu.c32
prompt 0
timeout 0

menu title PXE Menu

label 1
menu label ^1) Install CentOS 7.6
kernel centos/7.6/vmlinuz
append inst.ks=http://pxe-server/centos/7.6/ks.cfg initrd=centos/7.6/initrd.img

label 2
menu label ^2) Install Ubuntu 16.04
kernel ubuntu/16.04/linux
append ks=http://pxe-server/ubuntu/16.04/ks.cfg initrd=ubuntu/16.04/initrd.gz
